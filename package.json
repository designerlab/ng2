{
  "name": "ng2-fundamentals-demo",
  "version": "1.0.0",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run watch-css\" \"npm run server\" ",
    "server": "node node_modules/ng2f-server/server.js",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "build-css": "node-sass -w -r app/scss/ -o   app/css/",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "build": "npm run build:scss"
  },
