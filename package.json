    "rxjs": "5.0.1",
    "systemjs": "0.19.40",
    "toastr": "^2.1.2",
    "zone.js": "0.8.4"
  },
  "devDependencies": {
    "@types/core-js": "0.9.34",
    "@types/node": "6.0.46",
